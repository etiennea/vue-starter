    "format": "prettier --ignore-path .gitignore --write ./**/*.{js,ts,vue} src",
    "test": "cross-env NODE_ENV=test mocha-webpack --webpack-config build/webpack.tests.js --require tests/setup.js tests/**/*.spec.{js,ts}",
